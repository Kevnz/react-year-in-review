
{
	"extends": ["@kev_nz/eslint-config"],
	"parser": "babel-eslint",
	"rules": {
    "spellcheck/spell-checker": [
      1,
      {
        "comments": true,
        "strings": true,
        "identifiers": true,
        "lang": "en_US",
        "skipWords": [
          "dict",
          "aff",
          "hunspellchecker",
          "hunspell",
          "utils",
          "quickhit",
          "todos",
          "checkbox",
					"react-dom",
					"decrement"
        ],
        "skipIfMatch": [
          "http://[^s]*",
          "^[-\\w]+\/[-\\w\\.]+$",
          "^./quickhit"
        ],
        "minLength": 3
      }
    ]
  }
}